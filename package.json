{
  "name": "vue-next-libs",
  "version": "0.1.0",
  "main": "lib/umd/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/typing/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 4200",
    "build:demo": "vue-cli-service build",
    "build:lib:cli": "vue-cli-service build --target lib --name VueNextUi --dest lib ./src/components/index.ts",
    "build:esm": "rollup --format esm -c build/rollup.config.js",
    "build:umd": "rollup --format umd -c build/rollup.config.js",
    "build:types": "tsc --project src/components/tsconfig.json",
    "build:lib": "npm run build:esm && npm run build:umd",
    "build": "npm run build:lib && npm run build:types"
  },
  "dependencies": {
    "async-validator": "^3.5.1",
    "core-js": "^3.6.5",
    "lodash-es": "^4.17.21",
    "vueuc": "^0.4.10"
  },
  "peerDependencies": {
    "vue": "^3.2.3"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/lodash-es": "^4.17.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "rollup": "^2.51.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.4.1",
    "typescript": "~4.1.5",
    "vue": "^3.2.3",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-tsc": "^0.2.3"
  }
}
